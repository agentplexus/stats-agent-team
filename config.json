{
  "llm": {
    "provider": "gemini",
    "model": "gemini-2.0-flash-exp"
  },
  "search": {
    "provider": "serper"
  },
  "observability": {
    "enabled": false,
    "provider": "opik",
    "project": "stats-agent-team"
  },
  "agents": {
    "research": {
      "url": "http://localhost:8001",
      "description": "Web search for statistics sources"
    },
    "synthesis": {
      "url": "http://localhost:8004",
      "description": "Extract statistics from URLs using LLM"
    },
    "verification": {
      "url": "http://localhost:8002",
      "description": "Validate statistics exist in sources"
    },
    "orchestrator": {
      "url": "http://localhost:8000",
      "description": "Coordinate research workflow"
    },
    "orchestrator-eino": {
      "url": "http://localhost:8003",
      "description": "Eino-based orchestrator"
    }
  },
  "a2a": {
    "enabled": true,
    "authType": "apikey"
  },
  "security": {
    "enabled": false,
    "minScore": 50,
    "requireEncryption": false
  },
  "secrets": {
    "provider": "env",
    "prefix": "",
    "region": ""
  }
}
